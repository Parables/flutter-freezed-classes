{
  "name": "flutter-freezed-classes",
  "version": "0.0.9",
  "description": "GraphQL Code Generator plugin to generate freezed classes from your GraphQL schema",
  "respository": {
    "type": "git",
    "url": "https://github.com/Parables/graphql-codegen-flutter-freezed-classes.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "files": [
    "dist/**/*.{js,ts}",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "jest --no-watchman",
    "build": "run-p build:*",
    "build:main": "tsc -p tsconfig.main.json",
    "build:module": "tsc -p tsconfig.module.json",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --write --list-different \"**/*.{ts,graphql,yml}\"",
    "prettier:check": "prettier --check \"**/*.{ts,graphql,yml}\"",
    "generate": "run-p build:* && graphql-codegen",
    "prepublish": "run-p build:*",
    "prepack": "bob prepack"
  },
  "keywords": [
    "gql",
    "generator",
    "freezed",
    "flutter",
    "immutable classes",
    "flutter's favorite",
    "graphql",
    "codegen",
    "dart",
    "node",
    "types",
    "typings"
  ],
  "author": {
    "email": "parables95@gmail.com",
    "name": "Parables Boltnoel",
    "url": "https://github.com/parables"
  },
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "graphql": "^16.3.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "ts-jest": "^27.1.4",
    "tslib": "~1.11.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.4.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/visitor-plugin-common": "^2.7.4",
    "@graphql-tools/utils": "^8.6.5",
    "bob-the-bundler": "^1.6.1",
    "change-case-all": "1.0.14",
    "tslib": "~1.11.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Parables/graphql-codegen-flutter-freezed-classes/issues"
  }
}
